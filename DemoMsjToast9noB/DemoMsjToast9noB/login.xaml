<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DemoMsjToast9noB.login">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="UIsrael - SQLite Semana 7"
                VerticalOptions="Center" 
                HorizontalOptions="CenterAndExpand" />
            <Entry x:Name="txtUsuario" Placeholder="Ingrese el Usuario"/>
            <Entry x:Name="txtContrasenia" Placeholder="Ingrese la contrasena"/>
            <Button x:Name="btnIniciar" Text="Iniciar sesion" Clicked="btnIniciar_Clicked"/>
            <Button x:Name="btnRegistrar" Text="Registrar" Clicked="btnRegistrar_Clicked"/>
            <Label Text="Elaborado por: Andres Rodriguez 9noB"
                VerticalOptions="Center" 
                HorizontalOptions="CenterAndExpand" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
    